{
  "private": true,
  "name": "toponym",
  "version": "1.0.0",
  "scripts": {
    "build": "dotnet restore Toponym.sln && dotnet build Toponym.sln && npm install && npm run ts-bundle && npm run less",
    "tslint": "tslint -c ./Toponym.Site/app/dev/tslint.cli.json -p ./tsconfig.json --type-check",
    "start": "start npm run server && start npm run ts:w && start npm run watch && exit",
    "clean": "gulp clean",
    "server": "cd ./Toponym.Site/ && dotnet run",
    "server:br": "cd ./Toponym.Site/ && dotnet build && dotnet run",
    "ts-bundle": "tsc && node ./Toponym.Site/app/dev/ts-bundle.js",
    "ts:w": "tsc -w",
    "less": "gulp less",
    "watch": "gulp watch"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.26",
    "angular": "^1.8.0",
    "angular-ui-bootstrap": "^2.5.6",
    "bootstrap": "^3.4.1",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@types/angular": "^1.7.0",
    "@types/angular-ui-bootstrap": "^0.13.47",
    "@types/angular-ui-router": "^1.1.40",
    "@types/bootstrap": "^3.4.0",
    "@types/jquery": "^3.3.38",
    "browser-sync": "^2.26.7",
    "fs-extra": "^9.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-strip-css-comments": "^2.0.0",
    "lesshat": "^4.1.0",
    "merge2": "^1.3.0",
    "normalize.css": "^8.0.1",
    "rollup": "^0.65.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "tslint": "~6.1.2",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-microsoft-contrib": "~6.2.0",
    "tslint-misc-rules": "~3.5.1",
    "typescript": "^3.9.2",
    "uglify-js": "^3.9.3"
  },
  "author": "Arkadi Chubrik <arkadi@chubrik.org> (http://www.chubrik.org/)",
  "license": "SEE LICENSE IN http://www.toponim.by/license",
  "repository": "bitbucket:chubrik/Toponym"
}
