{
  "private": true,
  "name": "toponym",
  "version": "1.0.0",
  "scripts": {
    "build": "dotnet restore && dotnet build && npm install && npm run ts-bundle && npm run less",
    "tslint": "tslint -c ./Toponym.Site/dev/tslint.cli.json -p ./tsconfig.json --type-check",
    "start": "start npm run server && start npm run ts:w && start npm run watch && exit",
    "clean": "gulp clean",
    "server": "cd ./Toponym.Site/ && dotnet run",
    "server:br": "cd ./Toponym.Site/ && dotnet build && dotnet run",
    "ts-bundle": "tsc && node ./Toponym.Site/dev/ts-bundle.js",
    "ts:w": "tsc -w",
    "less": "gulp less",
    "watch": "gulp watch"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.13",
    "angular": "^1.6.8",
    "angular-ui-bootstrap": "^2.5.6",
    "bootstrap": "^3.3.7",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "@types/angular": "^1.6.41",
    "@types/angular-ui-bootstrap": "^0.13.46",
    "@types/angular-ui-router": "^1.1.40",
    "@types/bootstrap": "^3.3.37",
    "@types/jquery": "^3.3.0",
    "browser-sync": "^2.23.6",
    "fs-extra": "^5.0.0",
    "gulp": "github:gulpjs/gulp",
    "gulp-clean-css": "^3.9.2",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^3.4.0",
    "gulp-strip-css-comments": "^2.0.0",
    "lesshat": "^4.1.0",
    "merge2": "^1.2.1",
    "normalize.css": "^7.0.0",
    "rollup": "^0.55.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "tslint": "^5.2.0",
    "tslint-eslint-rules": "^4.0.0",
    "tslint-microsoft-contrib": "^5.0.2",
    "tslint-misc-rules": "^3.0.0",
    "typescript": "~2.3.4",
    "uglify-js": "^3.3.9"
  },
  "author": "Arkadi Chubrik <arkadi@chubrik.org> (http://www.chubrik.org/)",
  "license": "SEE LICENSE IN http://www.toponim.by/license",
  "repository": "bitbucket:chubrik/Toponym"
}
