{
  "compilerOptions": {
    "target": "es2015",
    "lib": [ "es2015", "dom" ],
    "module": "es2015",
    "moduleResolution": "node",
    "declaration": false,
    "sourceMap": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "../dist/js"
  }
}
