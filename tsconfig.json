{
  "compilerOptions": {
    "target": "es2015",
    "lib": [ "es2015", "dom" ],
    "module": "es2015",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "declaration": false,
    "sourceMap": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": ".",
    "rootDir": "./Toponym.Site/app/ts",
    "outDir": "./Toponym.Site/$temp/js"
  }
}
